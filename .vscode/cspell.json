{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  // Version of the setting file.  Always 0.2
  "version": "0.2",
  // language - current active spelling language
  "language": "en",
  "ignorePaths": [
    ".git",
    ".gitignore",
    "*.lock",
    "*.css",
    "*.ts",
    "*.tsx",
    "*.json",
    "*.js",
    "*.yaml"
  ],
  // words - list of words to be always considered correct
  "words": [
    "Andr",
    "Auca",
    "aucac",
    "aucacoyan",
    "Cgreen",
    "cheatsheet",
    "Chrone",
    "clippy",
    "Codely",
    "codewars",
    "Creset",
    "devs",
    "Dobell",
    "docbook",
    "dotfiles",
    "epub",
    "eruber",
    "esbuild",
    "exa",
    "Flexbox",
    "githubusercontent",
    "gitmessage",
    "Gloomhaven",
    "hardlinks",
    "hashnode",
    "Intelli",
    "katas",
    "kintastic",
    "Krazy",
    "Krivda",
    "lightsaber",
    "luabundler",
    "Luanalysis",
    "Moonsharp",
    "mypy",
    "nolt",
    "omniraptor",
    "oneline",
    "pipenv",
    "pipx",
    "Pocock",
    "POSIX",
    "println",
    "pwebb",
    "pyenv",
    "pythonic",
    "reactjs",
    "refreshenv",
    "ripgrep",
    "rustup",
    "Shiffman",
    "stackoverflow",
    "subproject",
    "tabletopsimulator",
    "Thackston",
    "ttslua",
    "Unclickable",
    "vscode",
    "WakaTime",
    "walkthrough",
    "Winget",
    "wingetdotrun",
    "xmlbundle",
    "Zehir"
  ],
  // flagWords - list of words to be always considered incorrect
  // This is useful for offensive words and common spelling errors.
  // For example "hte" should be "the"
  "flagWords": [
    "hte"
  ]
}
